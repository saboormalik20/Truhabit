extends base
block content
  section.content
    .container-fluid
      .row.progresss.mt-2(style="display:none")
        .col-md-12
          h5 Uploading
          .progress
            .progress-bar(role='progressbar', style='width: 0%;', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100') 0%
      .row
        .col-xs-12.col-sm-12.col-md-12.col-lg-12
          .card
            .body
              .d-flex
                input#excelupload(type="file")
                button.btn.text-light.uploadexcel(style="background-color:#18ce0f") Upload
              .table-responsive
                table.table.table-hover.js-basic-example.contact_list
                  thead
                    tr
                      th Prefix
                      th Code
                  tbody
                    each item in excel
                      tr
                        td= item.prefix
                        td= item.code

                        

      #myDelModal.modal(style='padding-top: 200px')
        .modal-dialog
          .modal-content
            // Modal Header
            .modal-header
              h2.modal-title Confirm Delete
              button.close(type='button', data-dismiss='modal')
                | &times;
            // Modal body
            .modal-body
              p(style='font-size: 24px')
                | Are you sure you want to delete this user?
            // Modal footer
            .modal-footer
              button.btn.onebuta.btn-danger.pt-3.userdels.yes(type='button')
                span.noload Delete
              button.btn.btn-danger.pr-3.twobuta.d-none.text-light(type='button')
                span.noload
                .spinner-border.load.d-none(role='status', style="color:white")
                  span.sr-only    
              button.btn.btn-gray.no(type='button', data-dismiss='modal')
                | Cancel
      #myEdosModal.modal
        .modal-dialog.modal-dialog-centered
          .modal-content
            // Modal Header
            .modal-header
              h2.modal-title Edit User
              button.close(type='button', data-dismiss='modal')
                | &times;
            // Modal body
            .modal-body
              .form-group
                label Name
                input.form-control.edasname(type="text",placeholder="Please Enter Name")
              .form-group
                label Email
                input.form-control.edasemail.text-dark(type="text",placeholder="Please Enter Eamil",readonly,disabled)
              .form-group
                label Password
                input.form-control.edaspassword(type="text",placeholder="Please Enter Password")
              .form-group
                label Points
                input.form-control.edaspoints(type="number",placeholder="Please Enter Points")
              .form-group
                label Country
                select.inputCountry.form-control
                  option UK
                  option USA
                  option SOUTH AFRICA
            // Modal footer
            .modal-footer
              button.btn.onebutap.whattoupd.pt-3.userupds.yes.text-light(type='button',style="background-color:#224e41")
                span.noload Update
              button.btn.twobutap.d-none.text-light(type='button',style="background-color:#224e41; padding-right:10px")
                span.noloadp
                .spinner-border.loadp.d-none(role='status', style="color:white")
                  span.sr-only    
              button.btn.btn-gray.no(type='button', data-dismiss='modal')
                | Cancel
